// 16_frontrunning.cvl
rule noFrontrunSensitive(method f) {
    env e1; env e2;
    require e1.block.number == e2.block.number;  // Same block race
    uint256 stateBefore = sensitiveState();
    f(e1); f(e2);  // Two txns race
    assert sensitiveState() == stateBefore, "State changed unexpectedly in same block";
}
